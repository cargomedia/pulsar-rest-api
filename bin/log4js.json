{
  "replaceConsole": true,
  "appenders": [
    {
      "type": "logLevelFilter",
      "level": "INFO",
      "appender": {
        "type": "console"
      }
    },
    {
      "type": "logLevelFilter",
      "level": "DEBUG",
      "appender": {
        "type": "dateFile",
        "pattern": "-yyyy-MM-dd",
        "filename": "pulsar-rest-api.log",
        "layout": {
          "type": "messagePassThrough"
        }
      }
    }
  ]
}
